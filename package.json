{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "frontend-dev": "npm run frontend-development",
        "frontend-development": "cross-env process.env.section=frontend NODE_ENV=development node_modules/webpack/bin/webpack.js --progress  --config=node_modules/laravel-mix/setup/webpack.config.js",
        "frontend-watch": "cross-env process.env.section=frontend NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "frontend-watch-poll": "npm run frntend-watch -- --watch-poll",
        "frontend-prod": "npm run frontend-production",
        "frontend-production": "cross-env process.env.section=frontend NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "admin-dev": "npm run admin-development",
        "admin-development": "cross-env process.env.section=admin NODE_ENV=development node_modules/webpack/bin/webpack.js --progress  --config=node_modules/laravel-mix/setup/webpack.config.js",
        "admin-watch": "cross-env process.env.section=admin NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --config=node_modules/laravel-mix/setup/webpack.config.js",
        "admin-watch-poll": "npm run admin-watch -- --watch-poll",
        "admin-prod": "npm run admin-production",
        "admin-production": "cross-env process.env.section=admin NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "sass": "^1.49.9",
        "sass-loader": "^12.6.0",
        "svg-spritemap-webpack-plugin": "^4.4.0",
        "vue": "^2.6.14",
        "vue-loader": "^15.9.8",
        "vue-router": "^3.5.3",
        "vue-template-compiler": "^2.6.14"
    },
    "dependencies": {
        "@popperjs/core": "^2.11.4",
        "bootstrap": "^5.1.3",
        "element-ui": "^2.15.6",
        "jquery": "^3.6.0",
        "owl.carousel": "^2.3.4",
        "popper.js": "^1.16.1",
        "vue-data-tables": "^3.4.5",
        "vue-scrollto": "^2.20.0",
        "vue-simple-progress": "^1.1.1",
        "vuedraggable": "^2.24.3",
        "vuejs-title": "^1.0.16"
    }
}
